<Window x:Class="WFP_Project.Pages.ControlWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Control Panel" Height="540" Width="1080"
        Loaded="Window_Loaded">
    <Grid>
        <Button Name="insertButton" Content="Insert" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0" Width="90" Click="InsertButton_Click"/>
        <TextBox Name="forceTextBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="110,10,0,0" Width="120"/>
        <TextBox Name="repeate_1stTextBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="240,10,0,0" Width="120" PreviewKeyDown="NumbersOnlyTextboxes"/>
        <TextBox Name="weightTextBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="370,10,0,0" Width="120"/>
        <TextBox Name="repeate_2ndTextBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="500,10,0,0" Width="120" PreviewKeyDown="NumbersOnlyTextboxes"/>
        <TextBox Name="goalTextBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="630,10,0,0" Width="120"/>
        <TextBox Name="repeate_3rdTextBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="760,10,0,0" Width="120" PreviewKeyDown="NumbersOnlyTextboxes"/>
        <DataGrid Name="databaseDataGrid" HorizontalAlignment="Stretch" VerticalAlignment="Top" Margin="10,50,10,10"/>
    </Grid>
</Window>
