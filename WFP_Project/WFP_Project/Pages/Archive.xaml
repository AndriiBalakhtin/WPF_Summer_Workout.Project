<Window x:Class="WFP_Project.Pages.ArchiveWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Archive" Height="450" Width="800"
        Loaded="Windows_Load">
    <Window.Background>
        <SolidColorBrush Color="Transparent"/>
    </Window.Background>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>

        <TreeView x:Name="archiveTreeView" Grid.Column="0" Margin="10"/>

        <StackPanel Grid.Column="1" Margin="10">
            <Button Content="Create Folder" Click="CreateFolder_Click" Margin="0,0,0,10"/>
            <Button Content="Save Data" Click="SaveData_Click"/>
        </StackPanel>
    </Grid>
</Window>
